            var kfi = 1;
            function koordinateFormat() {
                if (kfi == 1) {
                    document.getElementById("kf0").innerHTML = "46°&nbsp;10,1520'N&nbsp;/&nbsp;14°&nbsp;28,4820'E";
                        kfi = 2;
                    }
                    else if (kfi == 2) {
                        document.getElementById("kf0").innerHTML = "46°&nbsp;10'&nbsp;9,12''N&nbsp;/&nbsp;14°&nbsp;28'&nbsp;28,92''E";
                        kfi = 3;
                    }
                    else if (kfi == 3) {
                        document.getElementById("kf0").innerHTML = "46.1692°N&nbsp;/&nbsp;14.4747°E";
                    kfi = 4;
                }
                else if (kfi == 4) {
                    document.getElementById("kf0").innerHTML = "46,1692°N,&nbsp;14,4747°E";
                        kfi = 1;
                    }
    }

            function kopiraj() {
                var tempInput = document.createElement("textarea");
                tempInput.value = document.getElementById("kf0").innerHTML.replace(/&nbsp;/g, " ");
                document.body.appendChild(tempInput);
                tempInput.select();
                tempInput.setSelectionRange(0, 99999);
                document.execCommand("copy");
                document.body.removeChild(tempInput);
                tooltip("Kopirano v odložišče...");

            }

    
</script>
        
