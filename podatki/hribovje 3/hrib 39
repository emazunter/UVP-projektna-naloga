        <script>
            var kfi = 1;
            function koordinateFormat() {
                if (kfi == 1) {
                    document.getElementById("kf0").innerHTML = "46°&nbsp;21,0168'N&nbsp;/&nbsp;14°&nbsp;24,2832'E";
                        kfi = 2;
                    }
                    else if (kfi == 2) {
                        document.getElementById("kf0").innerHTML = "46°&nbsp;21'&nbsp;1,01''N&nbsp;/&nbsp;14°&nbsp;24'&nbsp;16,99''E";
                        kfi = 3;
                    }
                    else if (kfi == 3) {
                        document.getElementById("kf0").innerHTML = "46.35028°N&nbsp;/&nbsp;14.40472°E";
                    kfi = 4;
                }
                else if (kfi == 4) {
                    document.getElementById("kf0").innerHTML = "46,35028°N,&nbsp;14,40472°E";
                        kfi = 1;
                    }
    }

            function kopiraj() {
                var tempInput = document.createElement("textarea");
                tempInput.value = document.getElementById("kf0").innerHTML.replace(/&nbsp;/g, " ");
                document.body.appendChild(tempInput);
                tempInput.select();
                tempInput.setSelectionRange(0, 99999);
                document.execCommand("copy");
                document.body.removeChild(tempInput);
                tooltip("Kopirano v odložišče...");

            }

    
</script>
        
    <div class="g2"><table><tr><td><b>Širina/Dolžina:</b>&nbsp;</td><td><span id="kf0">46,35028°N&nbsp;14,40472°E</span></td></tr><tr><td></td><td><div style="padding-top:2px;"><table><tr><td style="padding-right:7px;"><span title="Spremeni format koordinat" style="cursor:pointer;" onclick="koordinateFormat();"><img class="potpng" src="/slike/refresh.png" /></span></td><td style="padding-right:7px;"><span title="Kopiraj v odložišče" style="cursor:pointer;" onclick="kopiraj();"><img class="potpng" src="/slike/kopiraj.png" /></span></td><td><span><a href="/gora_zemljevid/storzic/85"><img title="Zemljevid" class="potpng" src="/slike/navodila_zemljevid.png" /></a></span></td></tr></table></div></td></tr></table></div>
    
