var sortT = 0;
        function sortiraj(id) {
            
            if (parseInt(id) == 5) {
                if (sortT == 5) {
                    id = 6;
                    sortT = 6;
                }
                else {
                    id = 5;
                    sortT = 5;
                }
            }
            if (parseInt(id) == 3)
            {
                if (sortT == 3) {
                    id = 4;
                    sortT = 4;
                }
                else {
                    id = 3;
                    sortT = 3;
                }
            }
            if (parseInt(id) == 1) {
                if (sortT == 1) {
                    id = 2;
                    sortT = 2;
                }
                else {
                    id = 1;
                    sortT = 1;
                }
            }
            var xmlHttp;
            try {
                xmlHttp = new XMLHttpRequest();
            }
            catch (e) {
                try {
                    xmlHttp = new ActiveXObject("Msxml2.XMLHTTP");
                }
                catch (e) {
                    try {
                        xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
                    }
                    catch (e) {
                        return false;
                    }
                }
            }
