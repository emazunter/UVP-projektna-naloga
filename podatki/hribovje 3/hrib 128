        <script>
            var kfi = 1;
            function koordinateFormat() {
                if (kfi == 1) {
                    document.getElementById("kf0").innerHTML = "46°&nbsp;22,5720'N&nbsp;/&nbsp;14°&nbsp;14,7774'E";
                        kfi = 2;
                    }
                    else if (kfi == 2) {
                        document.getElementById("kf0").innerHTML = "46°&nbsp;22'&nbsp;34,32''N&nbsp;/&nbsp;14°&nbsp;14'&nbsp;46,64''E";
                        kfi = 3;
                    }
                    else if (kfi == 3) {
                        document.getElementById("kf0").innerHTML = "46.3762°N&nbsp;/&nbsp;14.24629°E";
                    kfi = 4;
                }
                else if (kfi == 4) {
                    document.getElementById("kf0").innerHTML = "46,3762°N,&nbsp;14,24629°E";
                        kfi = 1;
                    }
    }

            function kopiraj() {
                var tempInput = document.createElement("textarea");
                tempInput.value = document.getElementById("kf0").innerHTML.replace(/&nbsp;/g, " ");
                document.body.appendChild(tempInput);
                tempInput.select();
                tempInput.setSelectionRange(0, 99999);
                document.execCommand("copy");
                document.body.removeChild(tempInput);
                tooltip("Kopirano v odložišče...");

            }

    
</script>
        
