        <script>
            var kfi = 1;
            function koordinateFormat() {
                if (kfi == 1) {
                    document.getElementById("kf0").innerHTML = "46°&nbsp;24,6036'N&nbsp;/&nbsp;13°&nbsp;48,6606'E";
                        kfi = 2;
                    }
                    else if (kfi == 2) {
                        document.getElementById("kf0").innerHTML = "46°&nbsp;24'&nbsp;36,22''N&nbsp;/&nbsp;13°&nbsp;48'&nbsp;39,64''E";
                        kfi = 3;
                    }
                    else if (kfi == 3) {
                        document.getElementById("kf0").innerHTML = "46.41006°N&nbsp;/&nbsp;13.81101°E";
                    kfi = 4;
                }
                else if (kfi == 4) {
                    document.getElementById("kf0").innerHTML = "46,41006°N,&nbsp;13,81101°E";
                        kfi = 1;
                    }
    }

            function kopiraj() {
                var tempInput = document.createElement("textarea");
                tempInput.value = document.getElementById("kf0").innerHTML.replace(/&nbsp;/g, " ");
                document.body.appendChild(tempInput);
                tempInput.select();
                tempInput.setSelectionRange(0, 99999);
                document.execCommand("copy");
                document.body.removeChild(tempInput);
                tooltip("Kopirano v odložišče...");

            }

    
</script>
        
