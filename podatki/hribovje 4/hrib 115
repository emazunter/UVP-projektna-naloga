        <script>
            var kfi = 1;
            function koordinateFormat() {
                if (kfi == 1) {
                    document.getElementById("kf0").innerHTML = "46°&nbsp;27,0840'N&nbsp;/&nbsp;14°&nbsp;13,3260'E";
                        kfi = 2;
                    }
                    else if (kfi == 2) {
                        document.getElementById("kf0").innerHTML = "46°&nbsp;27'&nbsp;5,04''N&nbsp;/&nbsp;14°&nbsp;13'&nbsp;19,56''E";
                        kfi = 3;
                    }
                    else if (kfi == 3) {
                        document.getElementById("kf0").innerHTML = "46.4514°N&nbsp;/&nbsp;14.2221°E";
                    kfi = 4;
                }
                else if (kfi == 4) {
                    document.getElementById("kf0").innerHTML = "46,4514°N,&nbsp;14,2221°E";
                        kfi = 1;
                    }
    }

            function kopiraj() {
                var tempInput = document.createElement("textarea");
                tempInput.value = document.getElementById("kf0").innerHTML.replace(/&nbsp;/g, " ");
                document.body.appendChild(tempInput);
                tempInput.select();
                tempInput.setSelectionRange(0, 99999);
                document.execCommand("copy");
                document.body.removeChild(tempInput);
                tooltip("Kopirano v odložišče...");

            }

    
</script>
        
